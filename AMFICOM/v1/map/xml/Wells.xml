			<map:image>images/well.gif</map:image>
			<map:name>ККС-5</map:name>
			<map:description>Допускается монтаж кабелей местных сетей всех емкостей и установка HPП аппаратуры ИКМ для ГТС</map:description>
			<map:topological>true</map:topological>
			<map:characteristics>
				<map:characteristic typecodename="wellcapacity" value="24"/>
