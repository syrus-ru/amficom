<ext:root xmlns:res="http://syrus.com/AMFICOM/extensions/resources/" xmlns:ext="http://syrus.com/AMFICOM/extensions/">
  <ext:extension id="com.syrus.AMFICOM.resources.ResourceHandler" xsi:type="res:resources" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
  	<res:resource handlerClass="com.syrus.AMFICOM.resources.FontResourceHandler" id="font" xsi:type="res:handler"/>
  	<res:resource handlerClass="com.syrus.AMFICOM.resources.InsetsResourceHandler" id="insets" xsi:type="res:handler"/>
  	<res:resource handlerClass="com.syrus.AMFICOM.resources.ImageResourceHandler" id="image" xsi:type="res:handler"/>
  	<res:resource handlerClass="com.syrus.AMFICOM.resources.ColorResourceHandler" id="color" xsi:type="res:handler"/>
  	<res:resource handlerClass="com.syrus.AMFICOM.resources.DimensionResourceHandler" id="dimension" xsi:type="res:handler"/>
  	
    <res:resource id="TEST_IMAGE" xsi:type="res:image">
      <res:filename>test.gif</res:filename>
    </res:resource>
    
    <res:resource id="TEST_IMAGE2" xsi:type="res:image">
      <res:filename>test2.gif</res:filename>
      <res:size height="16" width="16"/>
    </res:resource>
    
    <res:resource id="AComboBox.font" xsi:type="res:font">
      <res:fontname>Arial</res:fontname>
      <res:style>BoldItalic</res:style>
      <res:size>10</res:size>
    </res:resource>
    
    <res:resource id="insets.null" xsi:type="res:insets">
      <res:top>0</res:top>
      <res:left>0</res:left>
      <res:bottom>0</res:bottom>
      <res:right>0</res:right>
    </res:resource>
    
    <res:resource id="EMPTY_DIMENSION" xsi:type="res:dimension">
      <res:size height="0" width="0"/>
    </res:resource>
  </ext:extension>
</ext:root>
