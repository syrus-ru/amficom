		String[] group_ids = new String[0];

		ExecIterator exIt = null;

		ObjectPermissionAttributes_Transferable opa = null;

		Vector vector = new Vector();

		CategoryIdIterator ciIt = null;
		GroupIdIterator giIt = null;
		Vector vector2;

		try
		{
			#sql exIt = {
				select
						ID,
						RWX,
						OWNER_ID,
						NAME,
						CODENAME,

						CREATED,
						CREATED_BY,
						MODIFIED,
						MODIFIED_BY,

