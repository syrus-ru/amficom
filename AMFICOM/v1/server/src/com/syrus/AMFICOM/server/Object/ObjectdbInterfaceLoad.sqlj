							"",
							"",
							"",
							0,
							"",
							0,
							"",
							"");

				domain_ids = loadDomainLinks(It_id);

				d_t = new Domain_Transferable(
					It_id,
					It_name,
					It_codename,
					It_owner_id,
					It_description,
					It_created,
					It_created_by,
					It_modified,
					It_modified_by,
					It_domain_id,
					domain_ids,
					It_opa);

				vector.add(d_t);
