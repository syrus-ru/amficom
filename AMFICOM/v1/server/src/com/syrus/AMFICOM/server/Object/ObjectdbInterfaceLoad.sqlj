		{
			if(exIt != null) try {
			exIt.close();
			}catch(Exception exe) {}
			System.out.println("error loading execs: " + e.getMessage());
			e.printStackTrace();
			return null;
		}
		finally
		{
			try {
				exIt.close();
			}catch(Exception exe) {}
		}
		
		return opa;
	}

	static public String[] loadDomainLinks(String id)
	{
		String It_id = "";
		InnerIdIterator idIt = null;
		Vector vector = new Vector();
		String[] ids = new String[0];
		try
		{
			#sql idIt = {
				select INNER_DOMAIN_ID from AMFICOM.DOMAINLINKS
				where DOMAIN_ID = :id
			};
			while (idIt.next())
			{
				try { It_id = idIt.inner_domain_id(); }
				catch(SQLException e) { }
				if(It_id != null)
					vector.add(It_id);
			}
			idIt.close();
			ids = new String[vector.size()];
			vector.copyInto(ids);
		}
		catch(SQLException e)
		{
			if(idIt != null) try {
			idIt.close();
			}catch(Exception exe) {}
			System.out.println("error loading domain links: " + e.getMessage());
			e.printStackTrace();
		}
		finally
		{
			try {
				idIt.close();
			}catch(Exception exe) {}
		}
		
		return ids;

	}

