	static public int loadUserDescriptors(
			UserSeq_TransferableHolder userseq)
	{
		String It_id = "";
		String It_login = "";
		String It_type = "";

		String object_id = "";
		long last_login = 0;
		long logged = 0;
		int sessions = 0;
		Timestamp lastlogin;

		String[] category_ids = new String[0];
		String[] group_ids = new String[0];

		Vector vector = new Vector();

		User_Transferable u_t;
		User_Transferable[] u_ts;
		UserIterator uIt = null;

		CategoryIdIterator ciIt = null;
		GroupIdIterator giIt = null;
		Vector vector2;

		try
		{
			#sql uIt = {
				select
						ID,
						LOGIN,
						TYPE,
						OPERATOR_ID,
						OPERATIONAL_ID,
						SUBSCRIBER_ID,
						ORGANIZATION_ID
					from AMFICOM.USERS
			};

			while (uIt.next())
			{
				try { It_id = uIt.id(); }
				catch(SQLException e) { }
				if(It_id == null) It_id = "";
				try { It_login = uIt.login(); }
				catch(SQLException e) { }
				if(It_login == null) It_login = "";
				try { It_type = uIt.type(); }
				catch(SQLException e) { }
				if(It_type == null) It_type = "";
				if(It_type.equals("operatorprofile"))
				{
					try { object_id = uIt.operator_id(); }
					catch(SQLException e) { }
					try {
						#sql { select LAST_LOGIN into :lastlogin
								from AMFIcOM.OPERATORPROFILES
									where ID = :object_id };
						last_login = lastlogin.getTime();
					} catch(Exception e) { }
					logged = last_login;
					#sql { select COUNT(*) into :sessions
							from AMFIcOM.LOGGEDUSERS
								where USER_ID = :It_id };
				}
				else
				if(It_type.equals("operationalprofile"))
				{
					try { object_id = uIt.operational_id(); }
					catch(SQLException e) { }
				}
				else
				if(It_type.equals("subscriberprofile"))
				{
					try { object_id = uIt.subscriber_id(); }
					catch(SQLException e) { }
				}
				else
				if(It_type.equals("organizationprofile"))
				{
					try { object_id = uIt.organization_id(); }
					catch(SQLException e) { }
				}
				if(object_id == null) object_id = "";

				vector2 = new Vector();
				try
				{
					#sql ciIt = { select CATEGORY_ID from AMFICOM.USERCATEGORIES
							where USER_ID = :It_id };
					while (ciIt.next())
					{
						try { vector2.addElement(ciIt.category_id()); }
						catch(SQLNullException e) { }
					}
					ciIt.close();
				}
				catch(SQLException e)
				{
					ciIt.close();
					System.out.println("could not get category for user " + It_id + ": " + e.getMessage());
					e.printStackTrace();
				}
				finally
				{
					try {
						ciIt.close();
					}catch(Exception exe) {}
				}
				
				category_ids = new String[vector2.size()];
				vector2.copyInto(category_ids);
//				System.out.println("found " + vector2.size() + " categories for profile " + It_id);

				vector2 = new Vector();
				try
				{
					#sql giIt = { select GROUP_ID from AMFICOM.USERGROUPS
							where USER_ID = :It_id };
					while (giIt.next())
					{
						try { vector2.addElement(giIt.group_id()); }
						catch(SQLNullException e) { }
					}
					giIt.close();
				}
				catch(SQLException e)
				{
					giIt.close();
					System.out.println("could not get group for user " + It_id + ": " + e.getMessage());
					e.printStackTrace();
				}
				finally
				{
					try {
						giIt.close();
					}catch(Exception exe) {}
				}
				
				group_ids = new String[vector2.size()];
				vector2.copyInto(group_ids);
//				System.out.println("found " + vector2.size() + " groups for profile " + It_id);

				u_t = new User_Transferable(
					It_id,
					object_id,
					It_login,
					It_type,
					last_login,
					logged,
					sessions,
					category_ids,
					group_ids);

				vector.add(u_t);
			}// while(opIt)
			uIt.close();
			u_ts = new User_Transferable[vector.size()];
			vector.copyInto(u_ts);
			userseq.value = u_ts;

		}
		catch(SQLException e)
		{
			if(uIt != null) try {
			uIt.close();
			}catch(Exception exe) {}
			System.out.println("error loading users: " + e.getMessage());
			e.printStackTrace();
			return Constants.ERROR_LOADING;
		}
		finally
		{
			try {
				uIt.close();
			}catch(Exception exe) {}
		}
		
		return Constants.ERROR_NO_ERROR;
	}

