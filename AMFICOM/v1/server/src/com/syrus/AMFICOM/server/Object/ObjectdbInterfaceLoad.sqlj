	static public ObjectPermissionAttributes_Transferable loadPermissionAttributes
		(String id)
	{
		String It_id = "";
		String It_rwx = "";
		String It_owner_id = "";
		String It_name = "";
		String It_codename = "";

		long It_created = 0;
		String It_created_by = "";
