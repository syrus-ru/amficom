<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: SchemeProtoGroup.xsd,v 1.2 2005/08/24 07:45:17 stas Exp $
-->
<xsd:schema 
	targetNamespace="http://syrus.com/AMFICOM/scheme/xml"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:scheme="http://syrus.com/AMFICOM/scheme/xml"
	xmlns:gen="http://syrus.com/AMFICOM/general/xml"
	elementFormDefault="qualified">

  <xsd:import namespace="http://syrus.com/AMFICOM/general/xml"/>

  <xsd:include schemaLocation="SchemeProtoElement.xsd"/>

  <xsd:element name="schemeprotogroups" type="scheme:SchemeProtoGroups">

    <xsd:key name="schemeprotogroupuid">
      <xsd:selector xpath="scheme:schemeprotogroup"/>
      <xsd:field xpath="@uid"/>
    </xsd:key>

  </xsd:element>

  <xsd:complexType name="SchemeProtoGroups">
    <xsd:sequence>
      <xsd:element name="schemeprotogroup" type="scheme:SchemeProtoGroup" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SchemeProtoGroup">
    <xsd:all>
      <xsd:element name="uid" type="gen:UID" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="symbol" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="schemeprotogroups" type="scheme:SchemeProtoGroups" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="schemeprotoelements" type="scheme:SchemeProtoElements" minOccurs="0" maxOccurs="1"/>
    </xsd:all>
  </xsd:complexType>

</xsd:schema>
